import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		int X=sc.nextInt();
		int N=sc.nextInt();
		int requiredPlanes = (int)Math.ceil((double)N/100);
        if(requiredPlanes <= X){
                System.out.println(0);
            } else {
                System.out.println(requiredPlanes - X);
            }
		}

	}
}
