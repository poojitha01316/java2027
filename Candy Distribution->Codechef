import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes 
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		    int N=sc.nextInt();
		    int M=sc.nextInt();
		    if(N % M ==0 && (N/M) % 2 ==0){
		        System.out.println("YES");
		    }else{
		        System.out.println("NO");
		    }
		}

	}
}
